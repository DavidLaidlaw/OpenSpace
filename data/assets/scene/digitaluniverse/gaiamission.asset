local assetHelper = asset.require("util/asset_helper")

local textures = asset.syncedResource({
    Name = "Stars Textures",
    Type = "HttpSynchronization",
    Identifier = "stars-denver_textures",
    Version = 1
})

local speck = asset.syncedResource({
    Name = "Stars Speck Files",
    Type = "HttpSynchronization",
    Identifier = "stars-denver_speck",
    Version = 1
})

local colorLUT = asset.syncedResource({
    Name = "Stars Color Table",
    Type = "HttpSynchronization",
    Identifier = "stars-denver_colormap",
    Version = 1
})


local GaiaMission = {
    Name = "GaiaMission",
    Renderable = {
        Type = "RenderableGaiaStars",
        File = speck .. "/denver_stars.speck",
        Texture = textures .. "/halo.png",
        ColorMap = colorLUT .. "/denver_colorbv.cmap",
        ColorOption = "Color",
        Transparency = 1.0,
        ScaleFactor = 5,
        MinBillboardSize = 1,
    },
    GuiPath = "/GaiaMission/Denver_Stars"
}
assetHelper.registerSceneGraphNodesAndExport(asset, { GaiaMission })